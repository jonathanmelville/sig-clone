{
  "name": "signal-dashboard",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "@vercel/edge-config": "^1.4.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-scripts": "5.0.1",
    "zod": "^3.25.76"
  },
  "scripts": {
    "prestart": "node scripts/init-data.js",
    "start": "concurrently \"react-scripts start\" \"cd mcp-server && npm run dev\" --names \"frontend,mcp\" --prefix-colors \"blue,green\"",
    "start:frontend": "react-scripts start",
    "start:mcp": "cd mcp-server && npm run dev",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "init-data": "node scripts/init-data.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
